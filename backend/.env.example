# IncomeOS Backend Environment Variables
# Copy this file to .env and fill in your values

# App
DEBUG=true

# OpenAI
OPENAI_API_KEY=sk-your-key-here
OPENAI_MODEL_ROUTER=gpt-4o-mini
OPENAI_MODEL_SPECIALIST=gpt-4o-mini
OPENAI_MODEL_COMPLEX=gpt-4o

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Redis
REDIS_URL=redis://localhost:6379/0

# Stripe
STRIPE_SECRET_KEY=sk_test_your-key
STRIPE_WEBHOOK_SECRET=whsec_your-secret
STRIPE_PRICE_BUILDER=price_builder_id
STRIPE_PRICE_OPERATOR=price_operator_id
STRIPE_PRICE_CEO=price_ceo_id

# Clerk
CLERK_SECRET_KEY=sk_test_your-clerk-key
CLERK_PUBLISHABLE_KEY=pk_test_your-clerk-key

# CORS
CORS_ORIGINS=["http://localhost:3000"]
